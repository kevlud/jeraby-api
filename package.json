    "dev:populateDB": "ts-node src/database/populateDB.ts"
  },
  "author": "Matěj Nevlud",
  "license": "ISC",
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/multer": "^1.3.10",
    "@types/terminal-kit": "^1.28.0",
    "apollo-server-express": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "core-util-is": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6-alpha.6",
    "express-session": "^1.16.2",
    "graphql": "^14.0.2",
    "inversify": "^5.0.1",
    "moment": "^2.24.0",
    "mssql": "^5.1.0",
    "multer": "^1.4.2",
    "mysql2": "^1.7.0",
    "pg": "^7.3.0",
    "reflect-metadata": "^0.1.13",
    "terminal-kit": "^2.1.8",
    "ts-node-dev": "^1.0.0-pre.43",
    "ts-transformer-keys": "^0.4.0",
    "typeorm": "^0.2.19",
    "xlsx": "^0.15.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/graphql": "^14.0.1",
    "@types/mysql": "^2.15.7",
    "@types/node": "^10.11.3",
    "@types/stripe": "^6.0.0",
    "eslint": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "typescript": "3.1.1"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
